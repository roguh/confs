tryalias em 'emacs -nw'
tryalias emc 'emacsclient -nw --alternate-editor=""'
tryalias emacsc 'emacsclient --alternate-editor=""'
tryalias mv 'mv -i'
tryalias cp 'cp -i'
tryalias rm 'rm -i'
tryalias please sudo

tryalias ping 'prettyping --nolegend' ping
tryalias less "$PAGER" less
tryalias more "$PAGER" more

tryalias ls exa ls
tryalias l 'ls -Fa'
tryalias ll 'l -lh'
tryalias grep 'grep --color'
tryalias gzip pigz gzip

tryalias c ccat
tryalias exa 'exa --git'

tryalias trizen 'trizen --flip-results=1'

tryalias py python3 python
tryalias ipy ipython3 ipython

tryalias v nvim vim vis vi nano ed libreoffice openoffice 'wine MicrosoftOffice2003.exe' vim
tryalias e 'zathura --fork' evince mupdf mupdf-x11 'wine READER10.exe'
tryalias sx 'exec startx'

tryalias npm pnpm npm
